      "schema:sync": "npm run typeorm schema:sync -- -d src/data-source.ts",
      "migration:show": "npm run typeorm migration:show -- -d src/data-source.ts",
      "migration:generate": "npm run typeorm migration:show -- -d src/data-source.ts",
      "migrate": " npm run build && typeorm migration:generate src/migration/add-entity -d ./build/src/utils/data-source.js",
